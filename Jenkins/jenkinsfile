pipeline{
    agent{
        label 'dev'
    }
    stages{
        stage('hostname'){
            steps{
                sh '''
                    hostname
                '''
            }
        }
        stage('Memory'){
            steps{
                sh '''
                    free -h
                '''
            }
        }
        stage('uptime'){
            steps{
                sh '''
                    uptime
                '''
            }
        }
    }
}